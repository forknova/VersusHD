{
  "sections": {
    "main": {
      "type": "main-product",
      "blocks": {
        "vendor": {
          "type": "text",
          "settings": {
            "text": "{{ product.vendor }}",
            "text_style": "uppercase"
          }
        },
        "title": {
          "type": "title",
          "settings": {}
        },
        "subtitle": {
          "type": "text",
          "settings": {
            "text": "{{ product.metafields.descriptors.subtitle.value }}",
            "text_style": "subtitle"
          }
        },
        "price": {
          "type": "price",
          "settings": {}
        },
        "c67c7eaf-fa73-4b77-bd9e-60dff338de4d": {
          "type": "product_view_count",
          "settings": {
            "view_count_text": "people are viewing this product",
            "max_view_count": 11
          }
        },
        "description": {
          "type": "description",
          "settings": {
            "column": "right",
            "accordion_default": "open"
          }
        },
        "variant_picker": {
          "type": "variant_picker",
          "settings": {
            "picker_type": "button"
          }
        },
        "quantity_selector": {
          "type": "quantity_selector",
          "settings": {}
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_dynamic_checkout": true
          }
        },
        "f8196964-ba2c-4900-9c4b-48572a80fdeb": {
          "type": "accordion",
          "disabled": true,
          "settings": {
            "column": "right",
            "style": "open",
            "heading": "Description",
            "content": "<p><strong>The Shawan is a great powerful ally in desperate times<\/strong>. His skills on the Battlefield are unmatched.Affront all your challenges head on and turn fear into self empowerment with the C-1000 Alpha 2. Get Yours Now!!!<\/p><p><\/p>",
            "css_classes": "description"
          }
        },
        "4a3ef586-2916-4dd2-b3ed-c37d445c28a5": {
          "type": "accordion",
          "settings": {
            "column": "left",
            "style": "open",
            "heading": "Unique Attributes",
            "content": "",
            "css_classes": "attributes"
          }
        },
        "share": {
          "type": "share",
          "disabled": true,
          "settings": {
            "share_label": "Share"
          }
        }
      },
      "block_order": [
        "vendor",
        "title",
        "subtitle",
        "price",
        "c67c7eaf-fa73-4b77-bd9e-60dff338de4d",
        "description",
        "variant_picker",
        "quantity_selector",
        "buy_buttons",
        "f8196964-ba2c-4900-9c4b-48572a80fdeb",
        "4a3ef586-2916-4dd2-b3ed-c37d445c28a5",
        "share"
      ],
      "settings": {
        "enable_sticky_info": true,
        "hide_variants": false,
        "enable_video_looping": false
      }
    },
    "product-recommendations": {
      "type": "product-recommendations",
      "settings": {
        "heading": "You may also like",
        "image_ratio": "square",
        "show_secondary_image": true,
        "add_image_padding": false,
        "show_image_outline": true,
        "show_vendor": false,
        "show_rating": false
      }
    }
